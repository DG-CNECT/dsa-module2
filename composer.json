{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "cweagans/composer-patches": "^1.7",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/scout": "^9.8",
        "laravel/tinker": "^2.7",
        "laravel/vapor-core": "^2.26",
        "laravel/vapor-ui": "^1.7",
        "laraveldaily/laravel-charts": "^0.1.29",
        "league/flysystem-aws-s3-v3": "^3.0",
        "luilliarcec/laravel-table": "^4.0",
        "margatampu/laravel-teams-logging": "^1.2",
        "profclems/postman-collection-generator": "^1.2",
        "sebbmeyer/laravel-teams-connector": "^0.6.2",
        "sebbmeyer/php-microsoft-teams-connector": "^0.2.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.9",
        "subfission/cas": "^4.3",
        "symfony/intl": "^6.0",
        "timwassenburg/laravel-pivot-table-generator": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "brianium/paratest": "^6.4",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.1",
        "laravel-shift/blueprint": "^2.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patches": {
            "apereo/phpcas": {
                "Ignore and bypass Session": "patches/apereo-phpcas.diff"
            },
            "subfission/cas": {
                "ServerBaseUrl": "patches/subfission-cas.diff"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
